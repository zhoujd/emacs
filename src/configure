# Use a slightly smaller set of warning options for lib/.
nw=
nw="$nw -Wunused-macros"

  gl_warn_set=
  set x $WARN_CFLAGS; shift
  for gl_warn_item
  do
    case " $nw " in
      *" $gl_warn_item "*)
        ;;
      *)
        gl_warn_set="$gl_warn_set $gl_warn_item"
        ;;
    esac
  done
  GNULIB_WARN_CFLAGS=$gl_warn_set





